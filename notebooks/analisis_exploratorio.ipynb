    "   \"source\": [\n",
    "    \"import pandas as pd\\n\",\n",
    "    \"import seaborn as sns\\n\",\n",
    "    \"import matplotlib.pyplot as plt\\n\",\n",
    "    \"\\n\",\n",
    "    \"df = pd.read_csv('../data/ventas.csv', parse_dates=['Fecha'])\\n\",\n",
    "    \"df['Mes'] = df['Fecha'].dt.month\\n\",\n",
    "    \"\\n\",\n",
    "    \"display(df.describe(include='all'))\\n\",\n",
    "    \"\\n\",\n",
    "    \"fig, axes = plt.subplots(1, 2, figsize=(12, 4))\\n\",\n",
    "    \"sns.barplot(data=df, x='Region', y='Ventas', ax=axes[0])\\n\",\n",
    "    \"axes[0].set_title('Ventas por región')\\n\",\n",
    "    \"sns.lineplot(data=df, x='Mes', y='Ventas', marker='o', ax=axes[1])\\n\",\n",
    "    \"axes[1].set_title('Temporalidad de ventas')\\n\",\n",
    "    \"plt.tight_layout()\\n\",\n",
